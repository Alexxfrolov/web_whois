          <div class="header__menu">
            <nav class="navbar"><a class="navbar__link" href="#">API Documentation</a></nav>
          </div>
        </div>
      </div>
    </header>
  </div>
  <div class="page__content">
    <div class="container">
      <%= yield %>
    </div>
  </div>
  <div class="page__footer">
